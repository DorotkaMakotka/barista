{
  "version": 2,
  "github": {
    "enabled": false
  },
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/data/(.*).json", "dest": "/data/$1.json" },
    { "src": "/(.*).js", "dest": "/$1.js" },
    { "src": "/(.*).css", "dest": "/$1.css" },
    { "src": "/(.*).html", "dest": "/$1.html" }
  ]
}
